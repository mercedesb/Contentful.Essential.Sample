@model HomeViewModel

<div class="row">
    <div class="col-md-4">
        <ul class="list-group">
            @foreach (var room in Model.Rooms)
            {
                <li class="list-group-item">
                    <a href="/room/Index/@room.Sys.Id">@room.Name</a>
                </li>
            }
        </ul>
    </div>
</div>